alias docker_clear='sudo docker stop $(sudo docker ps -a -q);sudo docker rm $(sudo docker ps -a -q)'
alias git_graph='git log --pretty=oneline --graph --decorate --all'
alias run_backend_debug='systemctl stop tfplenum-backend; cd /opt/tfplenum/web/backend/; source ../tfp-env/bin/activate; python run.py'
alias run_frontend_debug='cd /opt/tfplenum/web/frontend; npm start'
alias open_mongo='mongo'
alias server_backup='/opt/tfplenum/web/setup/backup_tfplenum.sh'
alias server_resore='/opt/tfplenum/web/setup/restore_tfplenum.sh'
alias operator_token='/opt/tfplenum/web/tfp-env/bin/python3 /opt/sso-idp/gen_api_token.py --roles "operator"'
alias admin_token='/opt/tfplenum/web/tfp-env/bin/python3 /opt/sso-idp/gen_api_token.py --roles "controller-admin"'
alias maintainer_token='/opt/tfplenum/web/tfp-env/bin/python3 /opt/sso-idp/gen_api_token.py --roles "controller-maintainer"'