# This file describes the network interfaces available on your system
# For more information, see netplan(5).
network:
  version: 2
  renderer: networkd
  ethernets:
    {{ device_name }}:
      dhcp4: no
      addresses:
      - {{ ip }}/24
      gateway4: {{ gateway }}
      nameservers:
        addresses:
        - {{ dns_servers[0] }}
