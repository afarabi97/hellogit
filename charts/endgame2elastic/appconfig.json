{
    "id": "Endgame",
    "type": "chart",
    "node_affinity": "Server - Any",
    "formControls": [
        {
          "type": "textinput",
          "default_value": "endgame.lan",
          "description": "Enter Endgame hostname or IP",
          "required": true,
          "regexp": "^(([a-zA-Z0-9]|[a-zA-Z0-9][a-zA-Z0-9\\-]*[a-zA-Z0-9])\\.)*([A-Za-z0-9]|[A-Za-z0-9][A-Za-z0-9\\-]*[A-Za-z0-9])$",
          "name": "endgame_host",
          "error_message": "Enter valid hostname or IP"
        },
        {
          "type": "textinput",
          "default_value": "admin",
          "description": "Enter Endgame user name",
          "required": true,
          "regexp": "",
          "name": "endgame_username",
          "error_message": "Enter username"
        },
        {
          "type": "textinput",
          "default_value": "password",
          "description": "Enter Endgame password",
          "required": true,
          "regexp": "",
          "name": "endgame_password",
          "error_message": "Enter password"
        },
        {
          "type": "textinput",
          "default_value": "10",
          "description": "Enter in how many minutes you want Endgame to update logs",
          "required": true,
          "regexp": "^\\d+$",
          "name": "run_every",
          "error_message": "Enter valid number"
        },
        {
          "type": "invisible",
          "name": "node_hostname"
        }
    ]
}
