elastic_ingest_nodes: []
logstash_nodes: []
home_net: ["192.168.0.0/16"]
interfaces: ["eth0"]
image_name: tfplenum/zeek
image_tag: {{ component["app_version"] }}
affinity_hostname: {}
ids_log_rotate_interval: 300
zeek_log_path: /data/zeek
zeek_loggers: 1
zeek_workers: 8
zeek_scripts: /opt/tfplenum/zeek/scripts
zeek_intel_dat: /opt/tfplenum/zeek/intel.dat
zeek_custom_sig_path: /opt/tfplenum/zeek/custom.sig
kibana_fqdn: kibana.default.svc.cluster.local
kibana_port: 443
filebeat_image_name: beats/filebeat
filebeat_image_tag: {{ context["docker_versions"]["elastic_7_version"] }}
shards: {}
hard_disk_drive: false
log_types:
  - connection
  - ssh
  - ssl
  - x509
  - ocsp
  - tunnel
  - socks
  - dns
  - http
  - smb_cmd
  - smb_files
  - smb_mapping
  - files
  - ftp
  - ntlm
  - kerberos
  - radius
  - intel
  - weird
  - notice
  - capture_loss
  - stats
  - dce_rpc
  - dhcp
  - dnp3
  - dpd
  - irc
  - modbus
  - mysql
  - pe
  - rdp
  - rfb
  - sip
  - smtp
  - snmp
  - syslog
  - traceroute
  - ntp
  - signature
use_logstash: false
