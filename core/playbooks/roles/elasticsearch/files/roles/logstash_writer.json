{
  "cluster": [
    "manage_index_templates",
    "monitor",
    "manage_ilm",
    "manage_ml",
    "monitor_ml",
    "read_ilm",
    "manage_ingest_pipelines"
  ],
  "indices": [
    {
      "names": [
        "logstash-*",
        ".monitoring-*",
        "filebeat-*",
        "winlogbeat-*",
        "auditbeat-*",
        "*beat*"
      ],
      "privileges": [
        "write",
        "create",
        "create_doc",
        "delete",
        "create_index",
        "manage",
        "manage_ilm",
        "monitor",
        "view_index_metadata",
        "read",
        "index"
      ],
      "allow_restricted_indices": false
    }
  ],
  "applications": [
    {
      "application": "kibana-.kibana",
      "privileges": [
        "all"
      ],
      "resources": [
        "*"
      ]
    }
  ],
  "run_as": [],
  "metadata": {},
  "transient_metadata": {
    "enabled": true
  }
}
